{
  "groups": [
    {
      "name": "zblog.aop-log",
      "type": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "description": "切面日志配置."
    }
  ],
  "properties": [
    {
      "name": "zblog.aop-log.enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": true,
      "description": "是否启用切面日志功能."
    },
    {
      "name": "zblog.aop-log.async",
      "type": "java.lang.Boolean",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": true,
      "description": "是否开启异步日志写入."
    },
    {
      "name": "zblog.aop-log.record-param",
      "type": "java.lang.Boolean",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": true,
      "description": "是否记录接口请求参数."
    },
    {
      "name": "zblog.aop-log.record-result",
      "type": "java.lang.Boolean",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": false,
      "description": "是否记录接口返回值，建议关闭以避免日志过大."
    },
    {
      "name": "zblog.aop-log.max-param-length",
      "type": "java.lang.Integer",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": 65535,
      "description": "记录的最大参数长度，超过部分将被截断."
    },
    {
      "name": "zblog.aop-log.max-result-length",
      "type": "java.lang.Integer",
      "sourceType": "com.github.stazxr.zblog.log.autoconfigure.properties.LogProperties",
      "defaultValue": 65535,
      "description": "记录的最大返回长度，超过部分将被截断."
    }
  ]
}
